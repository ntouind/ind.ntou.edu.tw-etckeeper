restart_cmd=/etc/init.d/samba restart || (/etc/init.d/nmbd restart ; /etc/init.d/smbd restart)
sort_mode=0
start_cmd=/etc/init.d/samba start || (/etc/init.d/nmbd start ; /etc/init.d/smbd start)
